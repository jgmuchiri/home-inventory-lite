[31m-$config['sess_cookie_name']		= 'ci_session';[m
[31m-$config['sess_expiration']		= 7200;[m
[31m-$config['sess_expire_on_close']	= FALSE;[m
[31m-$config['sess_encrypt_cookie']	= FALSE;[m
[31m-$config['sess_use_database']	= FALSE;[m
[31m-$config['sess_table_name']		= 'ci_sessions';[m
[31m-$config['sess_match_ip']		= FALSE;[m
[31m-$config['sess_match_useragent']	= TRUE;[m
[31m-$config['sess_time_to_update']	= 300;[m
